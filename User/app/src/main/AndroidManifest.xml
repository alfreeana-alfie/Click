        <activity android:name="com.ketekmall.ketekmall.pages.Register_Seller_MainPage" />
        <activity android:name="com.ketekmall.ketekmall.pages.Register_Seller" />
        <activity android:name="com.ketekmall.ketekmall.pages.More_Details" />
